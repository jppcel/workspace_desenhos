package desenhos;

public class Des_A implements Desenhar {
	
	private int SIZE = 0;
	
	@Override
	public void setSIZE(int a) {
		SIZE = a;
	}

	public void desenhar() {
	for(int col = 0;col<SIZE;col++){
		for(int lin = 0;lin<SIZE;lin++){
			if(col<=lin){
				System.out.print("*");
			}
			if (col == SIZE) {
				System.out.print("\n")
			}
			
		}
	}
		
	}
		

}
