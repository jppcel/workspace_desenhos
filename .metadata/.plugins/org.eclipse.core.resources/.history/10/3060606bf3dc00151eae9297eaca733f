package desenhos;

public class Des_B implements Desenhar {

private int SIZE = 0;
	
	@Override
	public void setSIZE(int a) {
		SIZE = a;
	}

	public void desenhar() {
	
		for(int lin = 1;lin<=SIZE;lin++){
			for(int col = 1;col<=SIZE;col++){
				if(lin<col){
					System.out.print("*");
				}
				if ((col == lin) && (col > 0)) {
					System.out.println();
				}			
			}
		}
			
	}

}
