	package desenhos;

public class Des_P implements Desenhar {

	private int SIZE = 0;
	
	@Override
	public void setSIZE(int a) {
		SIZE = a;
	}

	public void desenhar() {
	
		for(int lin = 0;lin<SIZE;lin++){
			for(int col = 0;col<SIZE;col++){
				
				if ((col == lin) && (lin > 0)){
					System.out.println();
				}
				
				if(lin<=col){
					if (col<1){
					System.out.print(SIZE - lin);
				}
				
							
			}
		}
			
	}

}
 